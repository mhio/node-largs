{
  "name": "largs",
  "version": "0.1.0-0",
  "engines" : { "node" : ">=6.0.0" },
  "description": "A light weight command line args parser",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha -bw",
    "test:unit": "mocha test/unit*",
    "test:unit:watch": "mocha -w test/unit*",
    "test:int": "mocha test/int*",
    "test:int:watch": "mocha -w test/int*",
    "test:debug": "DEBUG='*' mocha debug",
    "test:coverage": "istanbul cover _mocha && istanbul check-coverage",
    "release:test": "set -uex; yarn test; npm version prerelease; npm publish --tag testing",
    "release:patch": "set -uex; rm -rf node_modules; yarn; yarn run test:coverage; npm version patch; git push; npm publish; git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deployable/node-largs.git"
  },
  "keywords": [
    "deployable",
    "module"
  ],
  "author": "Matt Hoyle <code at deployable.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deployable/node-largs/issues"
  },
  "homepage": "https://github.com/deployable/node-largs#readme",
  "devDependencies": {
    "@deployable/test-cli": "^0.2.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "istanbul": "~0.4.5",
    "mocha": "^3.1.2",
    "sinon": "^1.17.7"
  },
  "dependencies": {
    "@deployable/errors": "~0.10.0",
    "debug": "^2.6.2",
    "lodash.foreach": "^4.5.0",
    "lodash.padend": "^4.6.1"
  }
}
